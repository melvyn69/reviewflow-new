{
  "framework": "vite",
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/|_next/|static/|assets/|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}